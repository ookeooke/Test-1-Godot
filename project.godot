; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Test 1"
run/main_scene="res://scenes/ui/main_menu.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scenes/managers/game_manager.tscn"
ClickManager="*res://scripts/autoloads/click_manager.gd"
CameraEffects="*res://scripts/autoloads/camera_effects.gd"
SaveManager="*res://scripts/autoloads/save_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[layer_names]

2d_physics/layer_1="enemies"
2d_physics/layer_2="towers"
2d_physics/layer_3="projectiles"
2d_physics/layer_4="tower_range"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
